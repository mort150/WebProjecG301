{% extends 'base.html.twig' %}

{% block title %}Hello SubjectController!{% endblock %}

{% block body %}
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Controller</th>
        </tr>
    </thead>
    <tbody>
    {% for subject in subjects %}
        <tr>
            <th>{{subject.id}}</th>
            <th>{{subject.name}}</th>
            <th>
            <a href="">Edit</a>
            <a href="">Delete</a>
            </th>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}
